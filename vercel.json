{
  "version": 2,
  "functions": {
    "vercel_wsgi.py": {
      "runtime": "python3.10"
    }
  },
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ]
}
