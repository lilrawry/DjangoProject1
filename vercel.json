{
  "version": 2,
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/vercel_wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/vercel_wsgi.py"
    },
    {
      "src": "/rooms/(.*)",
      "dest": "/vercel_wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel_wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
