{
  "version": 2,
  "functions": {
    "vercel_wsgi.py": {
      "runtime": "python3.10"
    }
  },
  "builds": [
    {
      "src": "vercel_wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "vercel_wsgi.py"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/img/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "DjangoProject1.settings",
    "PYTHONUNBUFFERED": "1"
  }
}
